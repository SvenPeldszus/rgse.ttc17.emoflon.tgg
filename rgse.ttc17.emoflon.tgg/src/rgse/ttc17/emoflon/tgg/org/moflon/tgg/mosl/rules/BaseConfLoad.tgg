#using tcc.tgg.*
#using AttrCondDefLibrary.*

#rule BaseConfLoad #extends Base #with tcc.tgg

#source {
	 energy : ConformLoad {
		-LoadGroup->group
	}
	
	group : ConformLoadGroup {
		-SubLoadArea->area
	}
	area : SubLoadArea {
		-ControlArea->contArea
	}
	contArea : ControlArea {
		
	}
}
#target{
	++ trgConsumer : EnergyConsumer {
	}
}
#correspondence{
	
}
#attributeConditions {
	eq(contArea.mRID, trgConsumer.ID)
}
